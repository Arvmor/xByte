{
    "name": "xbyte-sdk",
    "version": "0.0.4",
    "description": "TypeScript SDK for xByte Infra - The pay-per-byte content monetization protocol.",
    "keywords": [
        "blockchain",
        "monetization",
        "web3",
        "x402",
        "xbyte"
    ],
    "homepage": "https://xbyte.sh/",
    "bugs": "https://github.com/Arvmor/xbyte/issues",
    "license": "MIT",
    "author": "Arvin Moravej <moravej.arvin@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/Arvmor/xbyte.git",
        "directory": "xbyte-web/xbyte-sdk"
    },
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist/**/*.js",
        "dist/**/*.d.ts",
        "README.md",
        "LICENSE"
    ],
    "engines": {
        "node": ">=18.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsc",
        "lint": "oxlint",
        "lint:fix": "oxlint --fix",
        "format": "oxfmt",
        "format:check": "oxfmt --check"
    },
    "dependencies": {
        "viem": "^2.44.4"
    },
    "devDependencies": {
        "@types/bun": "*"
    },
    "peerDependencies": {
        "typescript": ">=5.0.0"
    }
}
